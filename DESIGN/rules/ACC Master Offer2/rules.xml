<project>
    <name>ACC Master Offer2</name>
    <comment></comment>
    <modules>
        <module>
            <name></name>
            <rules-root path="rules/*.xlsx"/>
            <method-filter>
                <includes>
                    <value>.+getBehaviorRequestDataMinMax.+</value>
                    <value>.+BehaviorRequestDataValueList.+</value>
                    <value>.+getBehaviorRequestDataDefault.+</value>
                    <value>.+ Behavior\(.+\)</value>
                    <value>.+getBehaviorRequestDataApplicability.+</value>
                    <value>.+getBehaviorRequestDataRelationship.+</value>
                </includes>
            </method-filter>
        </module>
    </modules>
    <dependencies>
        <dependency>
            <name>Offer API</name>
            <autoIncluded>true</autoIncluded>
        </dependency>
    </dependencies>
    <properties-file-name-pattern>.*-%state%-%effectiveDate%-%startRequestDate%</properties-file-name-pattern>
    <properties-file-name-pattern>.*Tests</properties-file-name-pattern>
    <properties-file-name-pattern>.*Model</properties-file-name-pattern>
    <properties-file-name-pattern>.*API</properties-file-name-pattern>
</project>